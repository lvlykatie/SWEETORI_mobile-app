<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/editTextTextPassword3"
            android:layout_width="356dp"
            android:layout_height="38dp"
            android:background="@drawable/editText_bg"
            android:clickable="true"
            android:ems="10"
            android:fontFamily="@font/poppins"
            android:gravity="center_vertical"
            android:inputType="textPassword"
            android:paddingLeft="10dp"
            android:text="Your password"
            android:textColor="#9098B1"
            tools:layout_editor_absoluteX="27dp"
            tools:layout_editor_absoluteY="375dp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:background="@drawable/sign_button_bg"
            android:fontFamily="@font/poppins_semibold"
            android:text="Done"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="130dp"
            tools:layout_editor_absoluteY="522dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/signup_curve"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="-35dp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:text="Create new password"
            android:textColor="@color/color02"
            android:textSize="22sp"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="277dp" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:text="Setting up your new password"
            android:textSize="17sp"
            tools:layout_editor_absoluteX="76dp"
            tools:layout_editor_absoluteY="347dp" />

        <EditText
            android:id="@+id/editTextTextPassword4"
            android:layout_width="356dp"
            android:layout_height="38dp"
            android:background="@drawable/editText_bg"
            android:clickable="true"
            android:ems="10"
            android:fontFamily="@font/poppins"
            android:gravity="center_vertical"
            android:inputType="textPassword"
            android:paddingLeft="10dp"
            android:text="Your password"
            android:textColor="#9098B1"
            tools:layout_editor_absoluteX="27dp"
            tools:layout_editor_absoluteY="451dp" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:text="Confirm your new password"
            android:textSize="17sp"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="423dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/signup_curve"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-35dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="SWEETORI"
        android:textColor="#000000"
        android:textSize="64sp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="187dp" />

</androidx.constraintlayout.widget.ConstraintLayout>