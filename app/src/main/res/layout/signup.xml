<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Already have an account?"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="659dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sign In"
        android:textColor="@color/color01"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="659dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:background="@drawable/sign_button_bg"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sign Up"
        android:textAllCaps="false"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="601dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Create an account"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Let's Get Started"
        android:textColor="@color/color02"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="259dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/signup_curve"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-39dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="SWEETORI"
        android:textColor="#000000"
        android:textSize="64sp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="162dp" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="356dp"
        android:layout_height="38dp"
        android:background="@drawable/editText_bg"
        android:clickable="true"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="center_vertical"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:text="Username"
        android:textColor="#9098B1"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="378dp" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="356dp"
        android:layout_height="38dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/editText_bg"
        android:clickable="true"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="center_vertical"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:text="Your email"
        android:textColor="#9098B1"
        app:layout_constraintTop_toBottomOf="@+id/editTextText4"
        tools:layout_editor_absoluteX="27dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="356dp"
        android:layout_height="38dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/editText_bg"
        android:clickable="true"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="center_vertical"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        android:text="Your password"
        android:textColor="#9098B1"
        app:layout_constraintTop_toBottomOf="@+id/editTextText5"
        tools:layout_editor_absoluteX="27dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="Accept all terms and policies"
        android:textSize="12sp"
        android:textStyle="italic"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
        tools:layout_editor_absoluteX="43dp" />

</androidx.constraintlayout.widget.ConstraintLayout>